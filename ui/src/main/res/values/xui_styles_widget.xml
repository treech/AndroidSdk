<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:tools="http://schemas.android.com/tools">

    <!--*********************************************
    *                  MaterialDialog               *
    **********************************************-->
    <style name="MD_ActionButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">?attr/md_button_height</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="MD_ActionButton.Text" tools:ignore="NewApi">
        <item name="android:textSize">?attr/md_button_textsize</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:background">@null</item>
        <item name="android:minWidth">@dimen/md_button_min_width</item>
        <item name="android:paddingStart">@dimen/md_button_textpadding_horizontal</item>
        <item name="android:paddingEnd">@dimen/md_button_textpadding_horizontal</item>
    </style>

    <style name="MD_Light" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="md_divider">@color/md_divider_black</item>
        <item name="md_list_selector">@drawable/md_selector</item>
        <item name="md_btn_stacked_selector">@drawable/md_selector</item>
        <item name="md_btn_positive_selector">@drawable/md_btn_selector</item>
        <item name="md_btn_neutral_selector">@drawable/md_btn_selector</item>
        <item name="md_btn_negative_selector">@drawable/md_btn_selector</item>

        <item name="android:windowAnimationStyle">@style/MD_WindowAnimation</item>
        <item name="android:backgroundDimEnabled">true</item>

    </style>

    <style name="MD_Dark" parent="Theme.AppCompat.Dialog.Alert">
        <item name="md_divider">@color/md_divider_white</item>
        <item name="md_list_selector">@drawable/md_selector_dark</item>
        <item name="md_btn_stacked_selector">@drawable/md_selector_dark</item>
        <item name="md_btn_positive_selector">@drawable/md_btn_selector_dark</item>
        <item name="md_btn_neutral_selector">@drawable/md_btn_selector_dark</item>
        <item name="md_btn_negative_selector">@drawable/md_btn_selector_dark</item>

        <item name="android:windowAnimationStyle">@style/MD_WindowAnimation</item>
        <item name="android:backgroundDimEnabled">true</item>

    </style>

    <style name="MD_WindowAnimation">
        <item name="android:windowEnterAnimation">@anim/md_popup_enter</item>
        <item name="android:windowExitAnimation">@anim/md_popup_exit</item>
    </style>


    <!--*********************************************
    *                 PopupWindow Style             *
    **********************************************-->
    <style name="XUI.Animation" parent="@android:style/Animation" />

    <style name="XUI.Animation.PopUpMenu" />

    <style name="XUI.Animation.PopUpMenu.Center">
        <item name="android:windowEnterAnimation">@anim/pop_grow_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/pop_shrink_from_top</item>
    </style>

    <style name="XUI.Animation.PopUpMenu.Left">
        <item name="android:windowEnterAnimation">@anim/pop_grow_from_bottomleft_to_topright</item>
        <item name="android:windowExitAnimation">@anim/pop_shrink_from_topright_to_bottomleft</item>
    </style>

    <style name="XUI.Animation.PopUpMenu.Right">
        <item name="android:windowEnterAnimation">@anim/pop_grow_from_bottomright_to_topleft</item>
        <item name="android:windowExitAnimation">@anim/pop_shrink_from_topleft_to_bottomright</item>
    </style>

    <style name="XUI.Animation.PopDownMenu" />

    <style name="XUI.Animation.PopDownMenu.Center">
        <item name="android:windowEnterAnimation">@anim/pop_grow_from_top</item>
        <item name="android:windowExitAnimation">@anim/pop_shrink_from_bottom</item>
    </style>

    <style name="XUI.Animation.PopDownMenu.Left">
        <item name="android:windowEnterAnimation">@anim/pop_grow_from_topleft_to_bottomright</item>
        <item name="android:windowExitAnimation">@anim/pop_shrink_from_bottomright_to_topleft</item>
    </style>

    <style name="XUI.Animation.PopDownMenu.Right">
        <item name="android:windowEnterAnimation">@anim/pop_grow_from_topright_to_bottomleft</item>
        <item name="android:windowExitAnimation">@anim/pop_shrink_from_bottomleft_to_topright</item>
    </style>

    <!--EditSpinner-->
    <style name="EditSpinner" parent="EditText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">?attr/ms_item_height_size</item>
        <item name="android:textSize">?attr/xui_config_size_spinner_text</item>
        <item name="android:textColor">@color/xui_config_color_edittext_textcolor</item>
        <item name="android:background">@drawable/xui_config_bg_edittext</item>
    </style>


    <!--*********************************************
    *                Spinner                        *
    **********************************************-->
    <style name="Material.SpinnerStyle" parent="@style/Widget.AppCompat.Spinner">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">?attr/ms_item_height_size</item>
        <item name="android:textSize">?attr/xui_config_size_spinner_text</item>
        <item name="android:textColor">@color/xui_config_color_edittext_textcolor</item>
        <item name="ms_dropdown_height">wrap_content</item>
        <item name="ms_text_color">@color/ms_dropdown_item_textcolor</item>
        <item name="ms_background_selector">@drawable/xui_config_bg_edittext</item>
        <item name="ms_dropdown_bg">@drawable/ms_drop_down_bg_radius</item>
    </style>

    <!--系统默认Spinner样式-->
    <style name="SpinnerStyle" parent="@style/Widget.AppCompat.Spinner">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">?attr/ms_item_height_size</item>
        <item name="android:spinnerMode">dropdown</item>
        <item name="android:background">@drawable/xui_config_bg_spinner</item>
        <item name="android:popupBackground">@drawable/ms_drop_down_bg_radius</item>
        <item name="android:paddingEnd">@dimen/default_spinner_icon_padding_size</item>
    </style>

    <!--*********************************************
    *                  GuideCaseView                *
    **********************************************-->
    <style name="DefaultGuideCaseTitleStyle" parent="@android:style/Widget.TextView">
        <item name="android:textSize">30sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textAppearance">@style/TextAppearance.FontPath</item>
    </style>

    <!--*********************************************
    *            Common Widget Style                *
    **********************************************-->
    <!-- 提示更新小圆点 -->
    <style name="XUITipPoint">
        <item name="android:layout_width">@dimen/xui_tips_point_size</item>
        <item name="android:layout_height">@dimen/xui_tips_point_size</item>
        <item name="android:background">@drawable/xui_tips_point</item>
    </style>

    <!-- 提示更新new -->
    <style name="XUITipNew">
        <item name="android:layout_width">37dp</item>
        <item name="android:layout_height">17dp</item>
        <item name="android:background">@drawable/xui_icon_tip_new</item>
    </style>

    <style name="XUIGroupListView">
        <item name="xui_separatorStyle">normal</item>
    </style>

    <style name="XUICommonListItemView">
        <item name="android:paddingStart">?attr/xui_config_content_spacing_horizontal</item>
        <item name="android:paddingEnd">?attr/xui_config_content_spacing_horizontal</item>
        <item name="android:background">?attr/xui_list_item_bg_with_border_none_selector</item>
        <item name="xui_commonList_titleColor">@color/xui_config_color_gray_1</item>
        <item name="xui_commonList_detailColor">@color/xui_config_color_gray_5</item>
    </style>

    <style name="XUIGroupListSectionView">
        <item name="android:paddingStart">?attr/xui_config_content_spacing_horizontal</item>
        <item name="android:paddingEnd">?attr/xui_config_content_spacing_horizontal</item>
        <item name="android:paddingTop">
            @dimen/xui_group_list_section_header_footer_padding_vertical
        </item>
        <item name="android:paddingBottom">
            @dimen/xui_group_list_section_header_footer_padding_vertical
        </item>
    </style>

    <!-- RadiusImageView -->
    <style name="RadiusImageView">
        <item name="riv_border_width">0dp</item>
        <item name="riv_border_color">@color/xui_config_color_gray_4</item>
        <item name="riv_is_touch_select_mode_enabled">true</item>
    </style>

    <!-- TabSegment -->
    <style name="TabSegment">
        <item name="ts_selected_color">?attr/colorAccent</item>
        <item name="ts_normal_color">@color/xui_config_color_gray_5</item>
        <item name="ts_has_indicator">true</item>
        <item name="ts_indicator_height">@dimen/xui_tab_segment_indicator_height</item>
        <item name="android:textSize">@dimen/xui_tab_segment_text_size</item>
        <item name="ts_indicator_top">false</item>
        <item name="ts_icon_position">left</item>
    </style>

    <style name="TabSegment_CountView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">?attr/xui_tab_sign_count_view_minSize</item>
        <item name="android:background">?attr/xui_tab_sign_count_view_bg</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/xui_config_color_white</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingStart">?attr/xui_tab_sign_count_view_padding_horizontal</item>
        <item name="android:paddingEnd">?attr/xui_tab_sign_count_view_padding_horizontal</item>
        <item name="android:minWidth">?attr/xui_tab_sign_count_view_minSize</item>
        <item name="android:minHeight">?attr/xui_tab_sign_count_view_minSize</item>
    </style>

    <!-- PickerView -->
    <style name="picker_view_scale_anim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/picker_view_dialog_scale_in</item>
        <item name="android:windowExitAnimation">@anim/picker_view_dialog_scale_out</item>
    </style>

    <style name="picker_view_slide_anim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/picker_view_slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/picker_view_slide_out_bottom</item>
    </style>

    <!-- RulerView -->
    <style name="RulerView">
        <item name="rv_rulerHeight">50dp</item>
        <item name="rv_rulerToResultGap">30dp</item>
        <item name="rv_scaleGap">10dp</item>
        <item name="rv_scaleLimit">1</item>
        <item name="rv_scaleCount">10</item>

        <item name="rv_bgColor">@color/default_ruler_view_bg_color</item>
        <item name="rv_smallScaleColor">@color/default_ruler_view_small_scale_color</item>
        <item name="rv_midScaleColor">@color/default_ruler_view_mid_scale_color</item>
        <item name="rv_largeScaleColor">@color/default_ruler_view_large_scale_color</item>
        <item name="rv_scaleNumColor">@color/default_ruler_view_scale_num_color</item>
        <item name="rv_resultNumColor">@color/default_ruler_view_result_num_color</item>
        <item name="rv_unitColor">@color/default_ruler_view_unit_color</item>

        <item name="rv_smallScaleStroke">2dp</item>
        <item name="rv_midScaleStroke">3dp</item>
        <item name="rv_largeScaleStroke">5dp</item>

        <item name="rv_resultNumTextSize">20sp</item>
        <item name="rv_scaleNumTextSize">16sp</item>
        <item name="rv_unitTextSize">13sp</item>

        <item name="rv_showScaleResult">true</item>
        <item name="rv_isBgRoundRect">true</item>
        <item name="rv_roundRadius">10dp</item>
    </style>

    <!-- VerifyCodeEditText -->
    <style name="VerifyCodeEditText">
        <item name="vcet_number">4</item>
        <item name="vcet_width">@dimen/default_vcet_width</item>
        <item name="vcet_divider">@drawable/vcet_shape_divider</item>
        <item name="vcet_text_color">@color/xui_config_color_pure_black</item>
        <item name="vcet_text_size">@dimen/default_vcet_text_size</item>
        <item name="vcet_bg_focus">@drawable/vcet_shape_bg_focus</item>
        <item name="vcet_bg_normal">@drawable/vcet_shape_bg_normal</item>
        <item name="vcet_is_pwd">false</item>
        <item name="vcet_pwd_radius">@dimen/default_vcet_pwd_radius</item>
    </style>


    <!--MaterialSearchView-->
    <style name="MSV_ImageButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:paddingStart">?attr/search_icon_padding</item>
        <item name="android:paddingEnd">?attr/search_icon_padding</item>
    </style>

    <!--BottomSheet-->
    <style name="BottomSheet" parent="android:Theme.Dialog">
        <item name="android:backgroundDimAmount">?attr/xui_dialog_background_dim_amount</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/xui_config_color_transparent</item>
        <item name="android:windowAnimationStyle">@null
        </item><!-- 为了避免在有 NavigationBar 的手机上 Dialog 从 NavigationBar 底部上来。去掉 Dialog 的动画，使用 View 的动画。-->
        <item name="android:layout_width">match_parent</item>
    </style>

    <!--MiniLoadingView-->
    <style name="MiniLoadingView">
        <item name="mlv_loading_view_size">?attr/mini_loading_view_size</item>
        <item name="mlv_loading_view_color">@color/xui_config_color_gray_5</item>
    </style>

    <!--SwitchButton-->
    <style name="SwitchButtonMD">
        <item name="swb_thumbDrawable">@drawable/swb_md_thumb</item>
        <item name="swb_backColor">@color/swb_md_back_color</item>
        <item name="swb_thumbRangeRatio">1.5</item>
        <item name="swb_thumbMarginLeft">-16dp</item>
        <item name="swb_thumbMarginTop">-14dp</item>
        <item name="swb_thumbMarginRight">-16dp</item>
        <item name="swb_thumbMarginBottom">-14dp</item>
        <item name="swb_thumbWidth">@dimen/swb_md_thumb_ripple_size</item>
        <item name="swb_thumbHeight">@dimen/swb_md_thumb_ripple_size</item>
    </style>

    <!--BottomSheet-->
    <style name="BottomSheetTitleStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/xui_bottom_sheet_title_height</item>
        <item name="android:background">?attr/xui_bottom_sheet_title_bg</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">?attr/xui_bottom_sheet_list_item_padding_horizontal</item>
        <item name="android:paddingEnd">?attr/xui_bottom_sheet_list_item_padding_horizontal</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="BottomSheetListItemTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="BottomSheetTitleStyle.Center">
        <item name="android:gravity">center</item>
    </style>

    <style name="BottomSheetListItemTextStyle.Center">
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
    </style>

    <!--CircleProgressView-->
    <style name="CircleProgressView">
        <item name="cpv_start_progress">0</item>
        <item name="cpv_end_progress">60</item>
        <item name="cpv_start_color">@color/xui_config_color_light_orange</item>
        <item name="cpv_end_color">@color/xui_config_color_dark_orange</item>
        <item name="cpv_isTracked">false</item>
        <item name="cpv_track_width">@dimen/default_pv_trace_width</item>
        <item name="cpv_track_color">@color/default_pv_track_color</item>
        <item name="cpv_progress_duration">1200</item>
        <item name="cpv_progress_textVisibility">true</item>
        <item name="cpv_progress_textColor">?attr/colorAccent</item>
        <item name="cpv_progress_textSize">@dimen/default_pv_progress_text_size</item>
        <item name="cpv_animate_type">AccelerateDecelerateInterpolator</item>
        <item name="cpv_isFilled">false</item>
        <item name="cpv_circle_broken">true</item>
        <item name="cpv_isGraduated">false</item>
        <item name="cpv_scaleZone_width">@dimen/default_pv_zone_width</item>
        <item name="cpv_scaleZone_length">@dimen/default_pv_zone_length</item>
        <item name="cpv_scaleZone_corner_radius">@dimen/default_pv_zone_corner_radius</item>
        <item name="cpv_scaleZone_padding">@dimen/default_pv_zone_padding</item>
    </style>

    <!--HorizontalProgressView-->
    <style name="HorizontalProgressView">
        <item name="hpv_start_progress">0</item>
        <item name="hpv_end_progress">60</item>
        <item name="hpv_start_color">@color/xui_config_color_light_orange</item>
        <item name="hpv_end_color">@color/xui_config_color_dark_orange</item>
        <item name="hpv_isTracked">false</item>
        <item name="hpv_track_width">@dimen/default_pv_trace_width</item>
        <item name="hpv_track_color">@color/default_pv_track_color</item>
        <item name="hpv_progress_duration">1200</item>
        <item name="hpv_progress_textVisibility">true</item>
        <item name="hpv_progress_textColor">?attr/colorAccent</item>
        <item name="hpv_progress_textSize">@dimen/default_pv_progress_text_size</item>
        <item name="hpv_animate_type">AccelerateDecelerateInterpolator</item>
        <item name="hpv_corner_radius">@dimen/default_pv_corner_radius</item>
        <item name="hpv_text_padding_bottom">@dimen/default_pv_corner_radius</item>
        <item name="hpv_text_movedEnable">true</item>
    </style>

    <style name="EditText.Clear">
        <item name="cet_clearIcon">@drawable/xui_ic_default_clear_btn</item>
        <item name="cet_clearIconSize">21dp</item>
    </style>

    <style name="EditText.Password">
        <item name="pet_iconShow">@drawable/pet_icon_visibility_24dp</item>
        <item name="pet_iconHide">@drawable/pet_icon_visibility_off_24dp</item>
        <item name="pet_hoverShowsPw">false</item>
        <item name="pet_nonMonospaceFont">false</item>
        <item name="pet_enableIconAlpha">true</item>
    </style>

    <style name="EditText.Material">
        <item name="android:paddingStart">0dp</item>

        <item name="met_textColor">?attr/xui_config_color_input_text</item>
        <item name="met_textColorHint">?attr/xui_config_color_hint_text</item>
        <item name="met_underlineColor">?attr/xui_config_color_stroke</item>
        <item name="met_errorColor">?attr/xui_config_color_error_text</item>
        <item name="met_helperTextColor">?attr/xui_config_color_error_text</item>

        <item name="met_floatingLabelTextSize">?attr/xui_config_size_edittext_helper_text</item>
        <item name="met_bottomTextSize">?attr/xui_config_size_edittext_helper_text</item>

        <item name="met_floatingLabelPadding">?attr/xui_config_size_edittext_components_spacing
        </item>
    </style>

    <style name="EditText.Validator">
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>

        <item name="android:background">@drawable/xui_config_bg_edittext</item>
        <item name="vet_autoValidate">true</item>
        <item name="vet_showErrorIcon">true</item>
        <item name="vet_errorIcon">@drawable/xui_ic_default_tip_btn</item>
        <item name="vet_errorIconSize">21dp</item>
        <item name="vet_tipPosition">top</item>
    </style>

    <style name="EasyIndicator">
        <item name="indicator_width">0dp</item>
        <item name="indicator_height">45dp</item>
        <item name="indicator_textSize">14sp</item>
        <item name="indicator_select_textSize">14sp</item>
        <item name="indicator_selected_color">?attr/colorAccent</item>
        <item name="indicator_normal_color">@color/xui_config_color_black</item>
        <item name="indicator_line_show">true</item>
        <item name="indicator_line_height">3dp</item>
        <item name="indicator_line_color">?attr/colorAccent</item>
        <item name="indicator_bottom_line_color">?attr/xui_config_color_separator_dark</item>
        <item name="indicator_bottom_line_height">0dp</item>
        <item name="indicator_vertical_line_w">0dp</item>
        <item name="indicator_vertical_line_h">0dp</item>
        <item name="indicator_vertical_line_color">?attr/xui_config_color_separator_dark</item>
    </style>

    <style name="MultipleStatusView">
        <item name="msv_loadingView">@layout/msv_layout_loading_view</item>
        <item name="msv_errorView">@layout/msv_layout_error_view</item>
        <item name="msv_emptyView">@layout/msv_layout_empty_view</item>
        <item name="msv_noNetworkView">@layout/msv_layout_no_network_view</item>
    </style>

    <style name="StatefulLayout">
        <item name="stf_animationEnabled">true</item>
        <item name="stf_inAnimation">@android:anim/fade_in</item>
        <item name="stf_outAnimation">@android:anim/fade_out</item>
    </style>

    <style name="TitleBar">
        <item name="tb_barHeight">?attr/xui_actionbar_height</item>
        <item name="tb_immersive">?attr/xui_actionbar_immersive</item>
        <item name="tb_actionPadding">?attr/xui_actionbar_action_padding</item>
        <item name="tb_sideTextPadding">?attr/xui_actionbar_side_text_padding</item>
        <item name="tb_sideTextColor">?attr/xui_actionbar_text_color</item>
        <item name="tb_titleTextColor">?attr/xui_actionbar_text_color</item>
        <item name="tb_subTitleTextColor">?attr/xui_actionbar_text_color</item>
        <item name="tb_actionTextColor">?attr/xui_actionbar_text_color</item>
        <item name="tb_sideTextSize">?attr/xui_actionbar_action_text_size</item>
        <item name="tb_titleTextSize">?attr/xui_actionbar_title_text_size</item>
        <item name="tb_subTitleTextSize">?attr/xui_actionbar_sub_text_size</item>
        <item name="tb_actionTextSize">?attr/xui_actionbar_action_text_size</item>
        <item name="tb_leftImageResource">?attr/xui_actionbar_ic_navigation_back</item>
        <item name="tb_dividerColor">@color/xui_config_color_transparent</item>
        <item name="tb_dividerHeight">1dp</item>
        <item name="tb_useThemeColor">true</item>
    </style>

    <style name="CountDownButton">
        <item name="cdbt_countDownFormat">%ds</item>
        <item name="cdbt_countDown">60000</item>
        <item name="cdbt_countDownInterval">1000</item>
        <item name="cdbt_enableCountDown">true</item>
    </style>

    <style name="AutoFitTextView">
        <item name="aftv_enable">true</item>
        <item name="aftv_minTextSize">8sp</item>
        <item name="aftv_precision">0.5</item>
    </style>

    <style name="RippleView">
        <item name="rv_alpha">90</item>
        <item name="rv_frameRate">10</item>
        <item name="rv_rippleDuration">400</item>
        <item name="rv_color">@color/xui_config_color_white</item>
        <item name="rv_centered">false</item>
        <item name="rv_type">simpleRipple</item>
        <item name="rv_ripplePadding">0dp</item>
        <item name="rv_zoom">false</item>
